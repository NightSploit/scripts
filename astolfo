-- Scripts:

local function EFHNZG_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local TweenService = game:GetService("TweenService")
	wait(2)
	TweenService:Create(script.Parent,TweenInfo.new(1),{TextTransparency = 0}):Play()
	wait(2.2)
	TweenService:Create(script.Parent,TweenInfo.new(1),{TextTransparency = 1}):Play()
end
coroutine.wrap(EFHNZG_fake_script)()
