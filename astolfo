local function OUPVH_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local TweenService = game:GetService("TweenService")
	wait(0.5)
	TweenService:Create(script.Parent,TweenInfo.new(1),{BackgroundTransparency = 0}):Play()
	wait(0.6)
	script.Parent:TweenSize(UDim2.new(0, 265,0, 391), "Out", "Quint",2)
	wait(4)
	script.Parent:TweenSize(UDim2.new(0, 265,0, 2), "Out", "Quint",1)
	wait(1)
	TweenService:Create(script.Parent,TweenInfo.new(1),{BackgroundTransparency = 1}):Play()
end
coroutine.wrap(OUPVH_fake_script)()
local function CBWI_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(CBWI_fake_script)()
local function PPVPTMV_fake_script() -- TextLabel_5.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_5)

			script.Parent.TextTransparency = 1
			for i = 1,10 do
				wait()
				script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
				script.Parent.Text = "お帰りなさい！主人, " .. game.Players.LocalPlayer.Name .. "."
			end
			wait(3)
			for i = 1,10 do
				wait()
				script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
	end
	wait(3.2)
	loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/NightSploit/scripts/master/astolfo"))()
end
coroutine.wrap(PPVPTMV_fake_script)()
local function KLZYH_fake_script() -- namething.LocalScript 
	local script = Instance.new('LocalScript', namething)

	script.Parent:TweenPosition(UDim2.new(0.356, 0,0.954, 0), "Out", "Quint",1)
	wait(5)
	script.Parent:TweenPosition(UDim2.new(0.356, 0,1.25, 0), "Out", "Quint",1)
end
coroutine.wrap(KLZYH_fake_script)()
